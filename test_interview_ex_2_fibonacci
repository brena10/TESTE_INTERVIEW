x = int(input("Digite um numero inteiro positivo: "))

fib_anterior = 0
fib_atual = 1

while fib_atual < x:
    fib_proximo = fib_anterior + fib_atual
    fib_anterior = fib_atual
    fib_atual = fib_proximo

if fib_atual == x:
    print(f"O número {x} pertence a sequencia de Fibonacci")
else:
    print(f"O número {x} não pertence a sequencia de Fibonacci")
